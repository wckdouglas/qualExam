@article{DAngelo:2003fj,
	Annote = {10.1074/jbc.M302244200},
	Author = {D'Angelo, Gisela and Duplan, Eric and Boyer, Nicole and Vigne, Paul and Frelin, Christian},
	Date = {2003/10/03},
	Date-Added = {2015-03-02 23:08:09 +0000},
	Date-Modified = {2015-03-02 23:08:14 +0000},
	Journal = {Journal of Biological Chemistry},
	Journal1 = {Journal of Biological Chemistry},
	Month = {10},
	Number = {40},
	Pages = {38183--38187},
	Title = {Hypoxia Up-regulates Prolyl Hydroxylase Activity: A FEEDBACK MECHANSIM THAT LIMITS HIF-1 RESPONSES DURING REOXYGENATION},
	Ty = {JOUR},
	Volume = {278},
	Year = {2003},
	Bdsk-Url-1 = {http://www.jbc.org/content/278/40/38183.abstract%20N2%20-%20The%20mechanism%20by%20which%20hypoxia%20induces%20gene%20transcription%20is%20now%20well%20established.%20Hypoxia%20reduces%20activity%20of%20prolyl%20hydroxylases%20(PHD)%20that%20hydroxylate%20specific%20proline%20residues%20in%20the%20oxygen-dependent%20degradation%20domain%20(ODD)%20of%20hypoxia-inducible%20factor-1%CE%B1%20(HIF-1%CE%B1).%20As%20a%20consequence,%20HIF-1%CE%B1%20accumulates%20and%20promotes%20hypoxic%20tolerance%20by%20activating%20gene%20transcription.%20This%20paper%20identifies%20the%20three%20forms%20of%20PHDs%20in%20rats%20and%20shows%20that%20a%20period%20of%20hypoxia%20selectively%20increases%20expression%20of%20PHD-2%20mRNAs%20levels.%20We%20developed%20assays%20for%20PHD%20activity%20that%20used%20(i)%20the%20peptide-specific%20conversion%20of%20labeled%202-oxoglutarate%20into%20succinate%20and%20(ii)%20the%20binding%20of%20the%20von%20Hippel-Lindau%20protein%20to%20a%20glutathione%20S-transferase-ODD%20fusion%20protein.%20The%20two%20assays%20indicated%20a%20low%20enzymatic%20activity%20in%20normoxic%20and%20hypoxic%20cells%20and%20a%20rapid%20increase%20during%20reoxygenation.%20We%20also%20developed%20hydroxyproline-specific%20antibodies%20that%20recognized%20hydroxylated%20forms%20of%20a%20fusion%20protein%20(ODD-green%20fluorescent%20protein)%20that%20combined%20the%20ODD%20domain%20of%20HIF-1%CE%B1%20and%20the%20green%20fluorescent%20protein.%20Using%20this%20antibody,%20we%20demonstrated%20that%20reoxygenation%20induced%20a%20rapid%20hydroxylation%20of%20Pro-564,%20which%20was%20followed%20by%20a%20massive%20degradation%20of%20the%20proteins.%20The%20results%20suggest%20that%20a%20hypoxic%20upregulation%20of%20PHD%20(presumably%20PHD-2)%20acts%20as%20a%20feedback%20mechanism%20to%20stop%20hypoxic%20responses%20in%20reoxygenated%20cells.%20We%20propose%20that%20proline%20hydroxylation%20might%20play%20a%20role%20in%20hypoxic%20preconditioning.}}

