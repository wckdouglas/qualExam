	J1 = {Front Oncol},
	Journal = {Frontiers in Oncology},
	Pages = {209},
	Publisher = {Frontiers Media S.A.},
	Title = {{Tumor budding cells, cancer stem cells and epithelial-mesenchymal transition-type cells in pancreatic cancer}},
	Ty = {JOUR},
	Volume = {2},
